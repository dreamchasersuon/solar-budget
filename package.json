{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "jest",
    "lint": "npx eslint *.js",
    "format": "npx prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx}\"",
    "snyk-protect": "snyk protect",
    "validate-ci-config": "circleci config validate -c .circleci/config.yml"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|sentry-expo|native-base))"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "react-native",
      "react",
      "expo",
      "react-native-gesture-handler"
    ]
  },
  "dependencies": {
    "@zeit/swr": "^0.1.2",
    "crypto-js": "^3.1.9-1",
    "expo": "^35.0.0",
    "expo-constants": "~7.0.1",
    "expo-font": "~7.0.0",
    "expo-image-picker": "~7.0.0",
    "expo-linear-gradient": "~7.0.0",
    "expo-local-authentication": "~7.0.0",
    "expo-localization": "~7.0.0",
    "expo-mail-composer": "~7.0.0",
    "expo-permissions": "~7.0.0",
    "expo-secure-store": "~7.0.0",
    "i18next": "^19.0.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "native-base": "^2.13.8",
    "prop-types": "^15.7.2",
    "react": "16.12.0",
    "react-i18next": "^11.2.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-chart-kit": "^4.1.0",
    "react-native-dropdownalert": "^4.2.0",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-picker-select": "^6.3.3",
    "react-native-reanimated": "^1.4.0",
    "react-native-screens": "^2.0.0-alpha.4",
    "react-native-svg": "9.13.3",
    "react-native-swiper": "^1.5.14",
    "react-navigation": "^4.0.10",
    "react-navigation-drawer": "^2.3.3",
    "react-navigation-stack": "^1.7.3",
    "react-navigation-tabs": "^2.6.0",
    "react-redux": "^7.1.1",
    "react-spring": "^8.0.27",
    "reanimated-bottom-sheet": "^1.0.0-alpha.18",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-persist-expo-securestore": "^2.0.0",
    "redux-starter-kit": "^1.0.1",
    "sentry-expo": "^2.0.1",
    "snyk": "^1.685.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.6.5",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-expo": "^7.1.0",
    "babel-preset-react-native": "^4.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "6.x",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-config-react-native": "^4.0.0",
    "eslint-plugin-flowtype": "^4.4.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-react-native": "^3.7.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "jest-expo": "^35.0.0",
    "jest-react-native": "^18.0.0",
    "prettier": "^1.19.1",
    "prettier-eslint-cli": "^5.0.0",
    "react-native-svg-transformer": "^0.14.2",
    "react-native-testing-library": "^1.11.1",
    "react-native-unimodules": "^0.6.0",
